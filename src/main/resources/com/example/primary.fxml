<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane fx:id="Primary" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.PrimaryController">
    <!-- Container for uploading images -->
    <VBox fx:id="uploadContainer" AnchorPane.leftAnchor="10.0" AnchorPane.topAnchor="10.0">
        <!-- UI elements for image uploading -->
        <Button onAction="#uploadImage" text="Upload Image" />
    </VBox>

    <!-- Container for displaying existing images -->
    <FlowPane fx:id="imageDisplayContainer" AnchorPane.leftAnchor="10.0" AnchorPane.topAnchor="100.0">
        <!-- Existing images will be displayed here -->
    </FlowPane>
</AnchorPane>
